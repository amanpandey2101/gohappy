                                <script>
                                    function showData(){
                                        const url1 = "/order-history/getdata";
                                        fetch(url1)
                                            .then(function (response) {
                                                return response.json();
                                            })
                                            .then(function (data) {
                                                appendData1(data);
                                                $(function () {
                                                    $("#table_id").DataTable({
                                                      "responsive": true, 
                                                      "autoWidth": true,
                                                      "buttons": ["excel"],
                                                      "ordering": true,
                                                      "paging": true,
                                                      "lengthChange": true,
                                                      "searching": true,
                                                      "info": true,
                                                    }).buttons().container().appendTo('#example1_wrapper .col-md-6:eq(0)');
                                                });
                                            })
                                            .catch(function (err) {
                                                console.log('error: ' + err);
                                            });
                                    }
                                
                                    function appendData1(data){

                                        for(var i=0; i<data.length; i++){

                                            $('#tbody_id').append(
                                                '<tr>' +
                                                    '<td>'+ parseInt(i+1) +'</td>' +
                                                    '<td>'+ data[i].building_name +'</td>' +
                                                    '<td>'+ data[i].room_rent +'</td>' +
                                                    '<td>'+ data[i].discount +'</td>' +
                                                    '<td>'+ data[i].actual_amount +'</td>' +
                                                    '<td>'+ data[i].name +'</td>' +
                                                    '<td><a href="mailto: '+ data[i].email +'">'+ data[i].email +'</a></td>' +
                                                    '<td><a href="tel: '+ data[i].number +'">'+ data[i].number +'</a></td>' +
                                                    '<td>' +
                                                        '<a href="#" id="'+ data[i].id +'" onclick="view(this.id)" class="btn btm-sm btn-info waves-effect"data-toggle="modal" data-target="#edit_view1"><i class="fa fa-eye"></i></a>' +
                                                    '</td>' +
                                                '</tr>'
                                            );
                                        }
                                    }
                                </script>
