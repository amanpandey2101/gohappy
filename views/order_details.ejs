                        <!-- sample modal content -->

                        <script>
                            var request_data;
                        
                            function view(clicked_id){
                            
                                request_data = clicked_id;
                            
                                console.log(request_data);
                            
                                const url2 = "/order/child/details?client_id=" + request_data;
                            
                                fetch(url2)
                            
                                .then(function (response) {
                                    return response.json();
                                })
                                .then(function (data) {

                                    $("#client_child_id tr").remove();

                                    for(var i=0; i<data.length; i++){
                                        $('#client_child_id').append(
                                            '<tr>' +
                                                '<th>Child Name:</th>' +
                                                '<td>'+ data[i].child_name +'</td>' +
                                                '<th>Child Age:</th>' +
                                                '<td>'+ data[i].child_age +'</td>' +
                                            '</tr>'
                                        );
                                    }
                                })

                                const url3 = "/order/client/details?client_id=" + request_data;
                            
                                fetch(url3)
                            
                                .then(function (response) {
                                    return response.json();
                                })
                                .then(function (data) {
                                    $("#client_name_id").empty();
                                    $("#client_email_id").empty();
                                    $("#client_number_id").empty();
                                    $("#client_total_amount_id").empty();
                                    $("#client_total_adult_id").empty();

                                    $("#client_name_id").append(data[0].name);
                                    $("#client_email_id").append(
                                        '<a href="mailto: '+ data[0].email +'">'+ data[0].email +'</a>'
                                    );
                                    $("#client_number_id").append(
                                        '<a href="tel: '+ data[0].number +'">'+ data[0].number +'</a>'
                                    );
                                    $("#client_total_amount_id").append(data[0].actual_amount);
                                    $("#client_total_adult_id").append(data[0].adults);
                                })
                                .catch(function (err) {
                                    console.log('error: ' + err);
                                });           
                            }
                        </script>

