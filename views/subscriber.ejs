                <script>
                    var request_data;
                
                    function view(clicked_id){
                    
                        request_data = clicked_id;
                    
                        console.log(request_data);
                    
                        const url2 = "/subscriber/getdata/edit?id=" + request_data;
                    
                        fetch(url2)
                    
                        .then(function (response) {
                            return response.json();
                        })
                        .then(function (data) {
                            
                            $("#edit_id").empty();
                            $("#edit_date").empty();
                            $("#edit_email").empty();
                            

                            $("#edit_id").val(data[0].id);
                            $("#edit_date").val(data[0].date);
                            $("#edit_email").val(data[0].email);
                            
                            /*==================================================Delete Packages=======================================================*/
                            $("#building_delete_footer").empty();
    
                            $("#building_delete_footer").append(
                                '<button type="button" class="btn btn-light waves-effect" data-dismiss="modal"><i class="fas fa-window-close"></i>&nbsp;Close</button>' +
                                '<button type="button" id="'+ data[0].id +'" onclick="remove_building(this.id)" class="btn btn-outline-danger waves-effect" data-dismiss="modal"><i class="fas fa-trash"></i>&nbsp;Yes</button>'
                            );
    
                        })
                        .catch(function (err) {
                            console.log('error: ' + err);
                        });           
                    }
                </script>

